    "@types/babel__traverse": "^7.20.5",
    "tsup": "^8.0.2"
  },
  "dependencies": {
    "@babel/core": "^7.23.9",
    "@babel/parser": "^7.23.9",
    "@babel/traverse": "^7.23.9"
  },
  "publishConfig": {
    "access": "public"
  }
}
